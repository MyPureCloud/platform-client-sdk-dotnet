<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>Infoveave.PureCloudPlatform.Client.V2</PackageId>
    <IsPackable>true</IsPackable>
    <Version>1.0.0</Version>
    <Authors>Infoveave</Authors>
    <Description>Genesys PureCloud Platform API Client for .NET</Description>
    <RepositoryUrl>https://github.com/${{ github.repository_owner }}/genesys-client</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" />
	<PackageReference Include="RestSharp" />
	<PackageReference Include="WebSocketSharp" />
  </ItemGroup>
</Project>
